    reve(k,len(nums-1))   

#majority element(169)
class Solution:
    def majorityElement(self, nums: List[int]) -> int:
      c=0
      candidate=0
      for num in nums:
            if c==0:
               candidate=num
               c+=1
            elif candidate==num:
                c+=1
            else:

#add digit(258)
class Solution:
    def addDigits(self, num: int) -> int:
        while num>=10:
            sumi=0
            while num>0:
               rem=num%10
               sumi+=rem
               num=num//10
            num=sumi
        return num
