      return  candidate

#majority element 2(229)
class Solution:
    def majorityElement(self, nums: List[int]) -> List[int]:
        c1=0
        c2=0
        num1=num2=None
        for num in nums:
            if num==num1:
                c1+=1
            elif num==num2:
                c2+=1
            elif c1==0:
                num1=num
                c1=1
            elif c2==0:
                    num2=num
                    c2=1
            else:
                    c1-=1
                    c2-=1
        k=[]
        if nums.count(num1)>len(nums)//3:
                k.append(num1)
        if nums.count(num2)>len(nums)//3:
                k.append(num2)
        return k
#roman to integer(13)
class Solution:
    def romanToInt(self, s: str) -> int:
        values = {
            'I': 1,
            'V': 5,
            'X': 10,
            'L': 50,
            'C': 100,
            'D': 500,
            'M': 1000
        }

        total = 0
        prev = 0

        for ch in reversed(s):
            curr = values[ch]
            if curr < prev:
                total -= curr
            else:
                total += curr
            prev = curr

        return total
